<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myuw.ui.page1.page1Fragment"
        android:label="@string/title_home"
        tools:layout="@layout/webview_fragment" >
        <argument
            android:name="base_url"
            app:argType="string"
            android:defaultValue="https://my-test.s.uw.edu/" />
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.myuw.ui.page1.page1Fragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/webview_fragment" >
        <argument
            android:name="base_url"
            app:argType="string"
            android:defaultValue="https://my-test.s.uw.edu/profile/" />
    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.myuw.ui.page1.page1Fragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/webview_fragment" >
        <argument
            android:name="base_url"
            android:defaultValue="https://my-test.s.uw.edu/academics/"
            app:argType="string" />
    </fragment>
</navigation>