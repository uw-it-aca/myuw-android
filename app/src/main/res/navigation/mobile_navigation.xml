<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_base_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Home"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/nav_academics"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_academics_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Academics"
            app:argType="string" />
        <action
            android:id="@+id/action_nav_academics_to_nav_url_open2"
            app:destination="@id/nav_url_open"
            app:enterAnim="@anim/slide_right_to_left"
            app:exitAnim="@anim/slide_left_to_right" />
    </fragment>

    <fragment
        android:id="@+id/nav_husky_experience"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_husky_experience_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Husky Experience"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/nav_accounts"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_accounts_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Accounts"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/nav_notices"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_notices_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Notices"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/nav_profile"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_profile_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Profile"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/nav_academic_calendar"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_academic_calendar_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Academic Calendar"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/nav_resources"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_resources_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="Resources"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/nav_url_open"
        android:name="edu.uw.myuw_android.CommonWebViewFragment"
        android:label="{title}">
        <argument
            android:name="base_url"
            android:defaultValue="@myuw_base_url"
            app:argType="string" />
        <argument
            android:name="title"
            android:defaultValue="temp"
            app:argType="string" />
        <action
            android:id="@+id/action_nav_url_open_self"
            app:destination="@id/nav_url_open"
            app:enterAnim="@anim/slide_right_to_left"
            app:exitAnim="@anim/slide_left_to_right" />
    </fragment>

</navigation>